      "providers": [
        "server_loginbuddy",
        "not_for_public_client",
        "server_dynamic"
      ],
      "on_behalf_of": [{"token_type":  "invalid"}]
    },
    {
      "client_id": "clientIdForTestingLoginbuddyManagement",
      "client_secret": "myFancyAndSecureClientSecret",
